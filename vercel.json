{
    "version": 2,
    "builds": [
      {
        "src": "games/NumberRush/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "games/NumberRush/dist" }
      }
    ],
    "routes": [
      { "src": "/games/NumberRush/(.*)", "dest": "/games/NumberRush/dist/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  