{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "rewrites": [
      { "source": "/games/(.*)", "destination": "/games/$1" },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  
  